   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Indexed", "[", 
      RowBox[{"x", ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "i"}], "}"}]}], "]"}], ",", " ", 
     RowBox[{"{", 
