           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Indexed", "[", 
               RowBox[{"x", ",", " ", 
                RowBox[{"#", " ", "+", "n"}]}], "]"}], " ", "&"}], "/@", " ", 
             
             RowBox[{"Range", "[", 
              RowBox[{"0", ",", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"N", "-", "1"}], ")"}], "*", "N"}], ",", "N"}], 
              "]"}]}], ")"}]}], ")"}], "  ", ")"}], " ", ",", "  ", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", "N"}], "}"}]}], "]"}]}], ";", " ", "\n", 
     
     RowBox[{"dig", " ", "=", " ", 
      RowBox[{"{", " ", 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{"(", 
          RowBox[{"Plus", " ", "@@", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Indexed", "[", 
               RowBox[{"x", ",", " ", "#"}], " ", "]"}], " ", "&"}], "/@", 
             " ", 
             RowBox[{"Range", "[", 
              RowBox[{"1", ",", 
               RowBox[{"N", "*", "N"}], ",", 
               RowBox[{"N", "+", "1"}]}], "]"}]}], ")"}]}], ")"}], " ", ")"}],
         ",", 
        RowBox[{"(", " ", 
         RowBox[{"(", 
          RowBox[{"Plus", " ", "@@", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Indexed", "[", 
               RowBox[{"x", ",", " ", "#"}], " ", "]"}], " ", "&"}], "/@", 
             " ", 
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"N", "*", "N"}], ")"}], "-", 
                 RowBox[{"(", 
                  RowBox[{"N", "-", "1"}], ")"}]}], ")"}], ",", "N", ",", 
               RowBox[{"-", 
                RowBox[{"(", 
                 RowBox[{"N", "-", "1"}], ")"}]}]}], "]"}]}], ")"}]}], ")"}], 
         ")"}]}], "}"}]}], ";", "\n", 
     RowBox[{"grid", " ", "=", "  ", 
      RowBox[{"Join", "[", 
       RowBox[{"hoz", ",", "ver", ",", "dig"}], "]"}]}], ";", " ", "\n", 
